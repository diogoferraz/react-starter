{
  "name": "react-starter-project",
  "version": "1.0.0",
  "description": "ReactJS start project with webpack, babel, sass, minify and many other goodies",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:prod & node server.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --config webpack.config.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "lint:fix": "eslint --ext .js --ext .jsx . --fix",
    "lint": "eslint . --ext .js --ext .jsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors"
  },
  "engines": {
    "node": "14.16.0",
    "npm": "6.14.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diogoferraz/react-starter.git"
  },
  "keywords": [
    "Babel",
    "Webpack",
    "ReactJS",
    "Sass",
    "Unit",
    "Test"
  ],
  "author": "Diogo Ferraz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/diogoferraz/react-starter/issues"
  },
  "homepage": "https://github.com/diogoferraz/react-starter#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^7.0.0",
    "path": "^0.12.7",
    "redux-devtools": "^3.7.0",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  },
  "dependencies": {
    "axios": "^0.22.0",
    "body-parser": "^1.19.0",
    "ejs": "^3.1.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "express": "^4.17.3",
    "jest": "^27.2.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest.config.js"
  }
}
